{
  "cli": {
    "version": ">= 16.6.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "env": {
        "GOOGLE_MAPS_API_KEY": "$GOOGLE_MAPS_API_KEY",
        "STREAM_API_KEY": "$STREAM_API_KEY"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "GOOGLE_MAPS_API_KEY": "$GOOGLE_MAPS_API_KEY",
        "STREAM_API_KEY": "$STREAM_API_KEY"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "GOOGLE_MAPS_API_KEY": "$GOOGLE_MAPS_API_KEY",
        "STREAM_API_KEY": "$STREAM_API_KEY"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
